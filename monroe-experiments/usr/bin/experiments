#!/bin/bash

#User experiments PIDs
RUNNING_USER_EXPERIEMENTS=""
for pid in $(cat /experiments/user/*.pid 2> /dev/null)
do
        if [ -d "/proc/$pid" ]
        then
                RUNNING_USER_EXPERIEMENTS="$USER_EXPERIEMENTS $pid"
        fi
done
echo "$RUNNING_USER_EXPERIEMENTS"

[[ ! -z "$RUNNING_USER_EXPERIEMENTS" ]]
exit $?
